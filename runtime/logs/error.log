    [timezone_type] =&gt; 3
    [timezone] =&gt; America/New_York
)
DateTime Object
(
    [date] =&gt; 2018-07-26 21:00:00.000000
    [timezone_type] =&gt; 3
    [timezone] =&gt; Asia/Hong_Kong
)
